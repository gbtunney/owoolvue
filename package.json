{
  "name": "slate-vue-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Shopify Inc., Liron Navon",
  "description": "A barebones, Slate compatible, starting point for developing Shopify themes with vue.",
  "keywords": [
    "shopify",
    "theme",
    "vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liron-navon/slate-vue-starter.git"
  },
  "devDependencies": {
    "@shopify/slate-tools": "^1.0.0-beta.14",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-shopify": "^16.6.0",
    "babel-preset-vue": "^2.0.2",
    "breakpoint-sass": "^2.7.1",
    "breakpoint-slicer": "^2.0.0",
    "chroma-sass": "^1.2.6",
    "compass-mixins": "^0.12.10",
    "is-color": "^1.0.2",
    "mathjs": "^5.4.2",
    "modularscale-sass": "^3.0.8",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "randomcolor": "^0.5.4",
    "sass-bem-constructor": "^1.1.0",
    "sass-dry": "^1.0.0",
    "sass-toolkit": "^2.10.2",
    "sassdash": "^0.9.0",
    "sassytester": "^1.1.0",
    "schm": "^0.4.1",
    "schm-translate": "^0.4.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-loader": "^15.4.2",
    "vue-multiselect": "^2.1.3",
    "vue-numeric-input": "^1.0.5",
    "vue-template-compiler": "^2.5.21",
    "vuex-rest-api": "^2.10.0"
  },
  "scripts": {
    "start": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "build": "slate-tools build",
    "deploy": "slate-tools build && slate-tools deploy",
    "zip": "slate-tools build && slate-tools zip",
    "pattern-test": "node-sass  -r --watch -rw --include-path node_modules  --watch -rw src/styles/pattern-lab-test.scss src/styles/pattern-lab-test.css",
    "styles": "node-sass  -r --importer=node_modules/node-sass-tilde-importer  --watch -rw src/styles/testing.scss src/assets/testing.css"
  },
  "dependencies": {
    "json-loader": "^0.5.7",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0"
  }
}
